{
  "name": "papmap",
  "version": "0.0.1",
  "description": "A monitored Javascript Map",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts",
    "lint": "tsc",
    "test": "vitest run",
    "test:watch": "vitest",
    "release": "npm run build && changeset publish"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/papsavas/papmap.git"
  },
  "keywords": [
    "map",
    "monitor",
    "cache",
    "db",
    "sync",
    "data",
    "collection"
  ],
  "author": "Savvas Papageorgiadis",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/papsavas/papmap/issues"
  },
  "homepage": "https://github.com/papsavas/papmap#readme",
  "devDependencies": {
    "@changesets/cli": "^2.26.1",
    "@vitest/coverage-c8": "^0.31.4",
    "tsup": "^6.7.0",
    "typescript": "^5.0.4",
    "vitest": "^0.31.4"
  },
  "dependencies": {
    "@discordjs/collection": "^1.5.1"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=7.0.0"
  }
}
